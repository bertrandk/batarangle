{
  "name": "batarangle",
  "version": "0.0.1",
  "description": "Chrome Developer Tools Extension for inspecting Angular 2.0 applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/batarangle.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "dev tools",
    "chrome",
    "extension"
  ],
  "bugs": {
    "url": "https://github.com/rangle/batarangle/issues"
  },
  "homepage": "https://github.com/rangle/batarangle",
  "scripts": {
    "test": "./node_modules/ts-node/bin.js ./node_modules/tape/bin/tape 'tests/**/*.ts'",
    "test-watch": "./node_modules/watch/cli.js 'browserify tests/**/*.ts -p [ ./node_modules/tsify --noImplicitAny ] | tape-run | tnyan' src/ tests/"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "tap-nyan": "0.0.2",
    "tape": "^4.2.1",
    "tape-catch": "^1.0.4",
    "tape-run": "^1.1.0",
    "ts-node": "^0.2.4",
    "tsify": "^0.12.2",
    "typescript": "^1.6.2",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "angular2": "^2.0.0-alpha.40",
    "rx": "^4.0.1"
  }
}
